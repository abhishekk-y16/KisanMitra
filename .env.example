# Example environment variables for local development
# Copy this file to .env and fill values. Do NOT commit secrets.

# JWT secret used by backend auth
JWT_SECRET=please_change_this_secret

# Weather provider API key (optional; required for /api/weather_forecast)
WEATHER_API_KEY=

# When set to 1, backend will try to serve static frontend from frontend/out
SERVE_FRONTEND=0

# Frontend client base URL (used by Next during dev/build)
NEXT_PUBLIC_API_URL=http://127.0.0.1:8000
# Example keys (do NOT commit real secrets) -- copy to .env for local dev
GROQ_API_KEY=
DATAGOV_API_KEY=
DATAGOV_RESOURCE_ID=
EARTH_ENGINE_API_KEY=
EE_SERVICE_ACCOUNT_JSON=
